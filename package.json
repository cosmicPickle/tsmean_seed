{
  "name": "server.api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "report": "chromium-browser coverage/index.html > /dev/null 2>&1 &",
    "test": "NODE_ENV=development npm run test:server",
    "test:server": "NODE_ENV=development nyc mocha -r ts-node/register --timeout=120000 '**/*.spec.ts' && npm run report",
    "utest:server": "NODE_ENV=development mocha -r ts-node/register --timeout=120000",
    "watch:server": "webpack --watch --mode=development --config server/webpack.config.js",
    "start": "concurrently \"npm run server:dev\" \"npm run watch:server\" ",
    "server:dev": "webpack --mode=development --config server/webpack.config.js && nodemon server/dist/index.js",
    "prod": "webpack --mode=production && nodemon dist/index.js",
    "server:resource": "ts-node tools/resource.ts 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "command-line-args": "^5.0.2",
    "express": "^4.16.3",
    "joi": "^13.3.0",
    "jsonwebtoken": "^8.2.0",
    "md5": "^2.2.1",
    "mongodb": "^3.0.8",
    "path-to-regexp": "^2.2.1",
    "tslint": "^5.10.0",
    "uuid": "^3.2.1",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/express": "^4.11.1",
    "@types/joi": "^13.0.8",
    "@types/mocha": "^5.2.0",
    "@types/mongodb": "^3.0.18",
    "@types/sinon": "^4.3.3",
    "@types/winston": "^2.3.9",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.4",
    "nyc": "^11.7.3",
    "pluralize": "^7.0.0",
    "sinon": "^4.4.8",
    "ts-loader": "^4.3.0",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.3",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "**/*.spec.ts",
      "server/src/configuration",
      "server/src/index.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
